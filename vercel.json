{
  "builds": [
    {
      "src": "codingdidi/wsgi.py",
      "use": "@ardnt/vercel-python-wsgi",  # Use recommended builder
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",  # Replace with your specific route pattern if applicable
      "dest": "codingdidi/wsgi.py"
    }
  ]
}
